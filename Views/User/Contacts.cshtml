            $(window).on("touchstart touchmove mouseover click", ".menu a", function (e) {
                e.preventDefault();
                clearInterval(t);
                var steps = 25;
                var target = $($(this).attr("href")).next("li");
                if (target.length > 0) {
                    var scroller = $("#scrolling")[0];
