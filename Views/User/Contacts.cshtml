                    $("#filedetailcheckboxes1").html(shorassign);
                    $("#filedetailcheckboxes").html(html123);
                    $(".ftagedit1").show();
                    $('#unassignedtag').html(htmlunassign);




                });



            }
        });
    });

    </script>











